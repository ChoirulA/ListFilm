<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.choirul.listfilm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Update_Film"
            android:label="@string/title_activity_update__film"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".List_Film"
            android:label="@string/title_activity_list__film"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Add_Films"
            android:label="@string/title_activity_add__films"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetailFilm"
            android:label="@string/title_activity_detail_film"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>